"scripts": {
  "start": "concurrently \"npm run watch-sass\" \"react-scripts start\"",
  "build": "npm run sass && react-scripts build",
  "test": "react-scripts test",
  "eject": "react-scripts eject",
  "sass": "sass src/styles/index.scss src/styles/styles.css",
  "watch-sass": "sass --watch src/styles/index.scss:src/styles/styles.css",
  "postcss": "postcss src/styles/styles.css -o src/styles/styles.css"
},
